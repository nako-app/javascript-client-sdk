{
  "name": "nako-client-sdk",
  "version": "0.0.3",
  "description": "Nako's JavaScript client SDK",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "lint-staged": {
    "*.ts": "prettier --config=.prettierrc --write --ignore-unknown"
  },
  "scripts": {
    "build": "tsc",
    "generate-defs": "./node_modules/.bin/graphql-codegen",
    "prepare": "npm run build",
    "lint": "prettier --config=.prettierrc \"**/*.(js|ts)\" --write --ignore-unknown",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "pre-commit": "lint-staged",
    "test": "mocha -r ts-node/register **/*.test.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nako-app/javascript-client-sdk.git"
  },
  "keywords": [
    "nako"
  ],
  "author": "Nako",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nako-app/javascript-client-sdk/issues"
  },
  "homepage": "https://github.com/nako-app/javascript-clientsdk#readme",
  "dependencies": {
    "@apollo/client": "^3.3.11",
    "apollo-link": "^1.2.14",
    "cross-fetch": "^3.0.6",
    "graphql": "^15.5.0",
    "lodash": "^4.17.21",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.21.1",
    "@graphql-codegen/typescript": "^1.21.0",
    "@graphql-codegen/typescript-operations": "^1.17.14",
    "@types/chai": "^4.2.15",
    "@types/mocha": "^8.2.1",
    "@types/node": "^14.14.31",
    "chai": "^4.3.0",
    "husky": "^5.1.1",
    "lint-staged": "^10.5.4",
    "mocha": "^8.3.0",
    "pinst": "^2.1.6",
    "ts-node": "^9.1.1"
  },
  "files": [
    "dist/*"
  ]
}
